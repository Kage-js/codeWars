function solution(start, finish) {
    let shelf = start
    let jumpCounter = 0
    if (shelf != finish) {
        do {
            if (shelf + 3 <= finish) {
                shelf += 3
                jumpCounter += 1
            } else if (shelf + 3 > finish) {
                shelf += 1
                jumpCounter += 1
            }
        } while (shelf < finish)
    }
    console.log("The cat finished on shelf " + shelf)
    console.log("The cat Jumped " + jumpCounter + " times")
    return jumpCounter
}